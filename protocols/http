ports=80/tcp

[https]
title=Hypertext Transfer Protocol (over SSL)
description=HTTP is the foundation of data communication for the World Wide Web
ports=443/tcp

[http-alt]
title=Hypertext Transfer Protocol (Alternative)
description=HTTP is the foundation of data communication for the World Wide Web
ports=8080/tcp

[http-full]
title=Hypertext Transfer Protocol (Full)
description=HTTP is the foundation of data communication for the World Wide Web
ports=80,443/tcp

[http-extra]
title=Hypertext Transfer Protocol (Extra)
